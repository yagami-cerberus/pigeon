<div class="modal-header">
  <h4 class="modal-title"><%= t :'user.add_api_key'%></h4>
</div>
<div class="modal-body form-horizontal">
  <div class="form-group">
    <label class="col-xs-3 control-label">Key</label>
    <div class="col-xs-9 form-control-static"><%= @apikey.key %></div>
  </div>
  <div class="form-group">
    <label class="col-xs-3 control-label">Secret Key</label>
    <div class="col-xs-9 form-control-static"><%= @apikey.secret_key %></div>
  </div>
</div>
<div class="modal-footer">
  <button type="button" class="btn btn-default" onclick="javascript:window.location.reload()">Close</button>
</div>
